<?xml version="1.0" encoding="UTF-8"?>
<Template
		xmlns="http://www.example.org/prueba2" 
		xmlns:x="http://www.w3.org/2001/XMLSchema-instance" 
		x:schemaLocation="http://www.example.org/prueba2 xsd.xsd">
		<template-name>template1</template-name>
		<template-version>1</template-version>
		<template-description>error</template-description>
		<template-author>
			<author-name>eduardo</author-name>
			<author-email>buhid</author-email>	
			<author-phone>123523</author-phone>
		</template-author>
			<variables>
			<variable>
				<name>outputBaseDir</name>
				<default>""</default>
				<required>false</required>	
			</variable>
		
			<variable>
				<name>cardinality</name>
				<default>0</default>
				<required>false</required>
			
			</variable>
			
			<variable>
				<name>templateCount</name>
				<default>0</default>
				<required>false</required>	
			</variable>
		
			
			<variable>
				<name>ontologyCount</name>
				<default>0</default>
				<required>false</required>	
			</variable>

		</variables>
		<macros>
			<macro>
				<template>classes.vm</template>
				<for>class</for>
				<output>classes/${class.getIRI().getShortForm()}.txt</output>
			</macro>
			<macro>
				<template>classes2.vm</template>
				<for>class</for>
				<output>classes2/${ontology.getOntologyID().getOntologyIRI().get().getShortForm()}</output>
				 
			</macro>
			
			<macro>
				<template>propertyValues.vm</template>
				<for>objectProperties</for>
				<output>propertiesValuesOut</output>
				
			</macro>
			 <macro>
				<template>enumerations.vm</template>
				<for>enumeration</for>
				<output>enumerationsDirectory</output>
			</macro>
			<macro>
				<template>instances.vm</template>
				<for>instance</for>
				<output>instancesDirectory</output>
			</macro>
				<macro>
				<template>ontology.vm</template>
				<for>ontology</for>
				<output>ontologyOutput</output>
			 
			</macro>
			
			 
			<macro>
				<template>project.vm</template>
				<for>project</for>
				<output>projectOutput</output>
			</macro>
			  
			<macro>
				<template>subclass.vm</template>
				<for>subclass</for>
				<output>subclassDirectory</output>
			</macro>
			<macro>
				<template>superclass.vm</template>
				<for>superclass</for>
				<output>superclassDirectory</output>
			</macro>			
			
		</macros>
</Template>

