<?xml version="1.0" encoding="UTF-8"?>
<Template
		xmlns="http://www.example.org/prueba2"
		xmlns:x="http://www.w3.org/2001/XMLSchema-instance"
		x:schemaLocation="http://www.example.org/prueba2 xsd.xsd">
		<template-name>template1</template-name>
		<template-version>1</template-version>
		<template-description>error</template-description>
		<template-author>
			<author-name>eduardo</author-name>
			<author-email>buhid</author-email>
			<author-phone>123523</author-phone>
		</template-author>

		<variables>
			<variable>
				<name>outputBaseDir</name>
				<default></default>
				<required>false</required>
				<description>"variable to set outputBaseDir"</description>
			</variable>

			<variable>
				<name>targetOS</name>
				<default>Linux</default>
				<required>false</required>
				<description>"variable containing target operating system"</description>

			</variable>

			<variable>
				<name>templateCount</name>
				<default>0</default>
				<required>false</required>
				<description>"variable to set template count"</description>
			</variable>


			<variable>
				<name>ontologyCount</name>
				<default>0</default>
				<required>false</required>
				<description>"variable to set ontology count"</description>
			</variable>

		</variables>
		<macros>
			<macro>
				<template>classes.vm</template>
				<for>class</for>
				<output>classes/${class.getIRI().getShortForm()}</output>
				<imports>
					<FullyQualifiedName>package.example.one</FullyQualifiedName>
					<FullyQualifiedName>package.example.two</FullyQualifiedName>
				</imports>
			</macro>
			<macro>
				<template>classes2.vm</template>
				<for>class</for>
				<output>classes2/${class.getIRI().getFragment()}</output>
				<imports>
					<FullyQualifiedName>com.test.upm.protege.plugin</FullyQualifiedName>
					<FullyQualifiedName>com.test.upm.rest.plugin.StaticExample</FullyQualifiedName>
					<FullyQualifiedName>com.test.upm.codegenerator</FullyQualifiedName>
				</imports>
			</macro>

			 <macro>
				<template>enumerations.vm</template>
				<for>enumeration</for>
				<output>enumerationsDirectory</output>
				<imports>
					<FullyQualifiedName>org.apache.velocity.imports</FullyQualifiedName>
				</imports>
			</macro>
			<macro>
				<template>instances.vm</template>
				<for>instance</for>
				<output>instancesDirectory/${instance.getIRI().getFragment()}</output>
				<imports>
					<FullyQualifiedName>org.semanticweb.owlapi.model.AxiomType</FullyQualifiedName>
				</imports>
			</macro>

				<macro>
				<template>ontology.vm</template>
				<for>ontology</for>
				<output>ontologyOutput</output>
				<imports>
					<FullyQualifiedName>com.test.upm.codegenerator.test</FullyQualifiedName>
				</imports>

			</macro>

			<macro>
				<template>project.vm</template>
				<for>project</for>
				<output>projectOutput</output>
				<imports>
					<FullyQualifiedName>com.test.upm.codegenerator.test.projectTestStaticImport</FullyQualifiedName>
				</imports>
			</macro>


		</macros>
</Template>

